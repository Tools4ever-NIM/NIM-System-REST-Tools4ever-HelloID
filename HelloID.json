{
    "schema": {
        "crud_objects": {
            "users": {
                "operations": {
                    "UserList": {
                        "method": "get",
                        "call": {
                            "path": "/api/v1/users"
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    },
                    "UserCreate": {
                        "method": "post",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/users"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "lastLogin",
                            "links",
                            "isLocked",
                            "isDeleted",
                            "lockTime",
                            "roles",
                            "passwordLastSetTime",
                            "userGUID"
                        ],
                        "resource_mandatory": [
                            "username"
                        ]
                    },
                    "UserUpdate": {
                        "method": "put",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/users/{userGUID}"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "lastLogin",
                            "links",
                            "isLocked",
                            "isDeleted",
                            "lockTime",
                            "roles",
                            "passwordLastSetTime"
                        ],
                        "resource_mandatory": [
                            "userGUID"
                        ]
                    },
                    "UserDelete": {
                        "method": "delete",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/users/{userGUID}"
                        },
                        "semantics": "delete",
                        "resource_allowance_default": "prohibited",
                        "resource_prohibited": [
                            "lastLogin",
                            "links",
                            "isLocked",
                            "isDeleted",
                            "lockTime",
                            "roles",
                            "passwordLastSetTime"
                        ],
                        "resource_mandatory": [
                            "userGUID"
                        ]
                    },
                    "UserCredentialUpdate": {
                        "method": "post",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/users/addUserCredentials/{userGUID}"
                        },
                        "body": {
                            "username": "{username}",
                            "password": "{password}"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "prohibited",
                        "resource_prohibited": [
                            "lastLogin",
                            "links",
                            "isLocked",
                            "isDeleted",
                            "lockTime",
                            "roles",
                            "passwordLastSetTime"
                        ],
                        "resource_mandatory": [
                            "userGUID",
                            "username",
                            "password"
                        ]
                    },
                    "UserPhotoUpdate": {
                        "method": "put",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/users/{userGUID}/photo"
                        },
                        "body": {
                            "profilePhotoBase64": "{profilePhotoBase64}"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "prohibited",
                        "resource_prohibited": [
                            "lastLogin",
                            "links",
                            "isLocked",
                            "isDeleted",
                            "lockTime",
                            "roles",
                            "passwordLastSetTime"
                        ],
                        "resource_mandatory": [
                            "userGUID",
                            "profilePhotoBase64"
                        ]
                    }
                },
                "key": "userGUID",
                "resources": {
                    "userGUID": "string*",
                    "lastLogin": "string*",
                    "source": "string*",
                    "links": "string*",
                    "firstName": "string*",
                    "lastName": "string*",
                    "userName": "string*",
                    "managedByuserGUID": "string*",
                    "isEnabled": "boolean*",
                    "isLocked": "boolean*",
                    "isDeleted": "boolean*",
                    "mustChangePassword": "boolean*",
                    "lockTime": "string*",
                    "passwordLastSetTime": "string*",
                    "roles": [
                        "string*"
                    ],
                    "adsid": "string*",
                    "contactEmail": "string*",
                    "password": "string*",
                    "profilePhotoBase64": "string*",
                    "userAttributes": {
                        "ssoassertion001": "_:string*",
                        "thumbnailphoto": "_:string*",
                        "phonenumber": "_:string*",
                        "department": "_:string*",
                        "description": "_:string*",
                        "objectguid": "_:string*",
                        "contactemailtemp": "_:string*",
                        "samaccountname": "_:string*",
                        "employeeid": "_:string*",
                        "employeetype": "_:string*",
                        "extensionattribute1": "_:string*",
                        "extensionattribute2": "_:string*",
                        "extensionattribute3": "_:string*",
                        "extensionattribute4": "_:string*",
                        "extensionattribute5": "_:string*",
                        "extensionattribute6": "_:string*",
                        "extensionattribute7": "_:string*",
                        "extensionattribute8": "_:string*",
                        "extensionattribute9": "_:string*",
                        "extensionattribute10": "_:string*",
                        "extensionattribute11": "_:string*",
                        "extensionattribute12": "_:string*",
                        "extensionattribute13": "_:string*",
                        "extensionattribute14": "_:string*",
                        "extensionattribute15": "_:string*",
                        "title": "_:string*",
                        "azureimmutableid": "_:string*",
                        "language": "_:string*",
                        "qrhash": "_:string*",
                        "qr": "_:string*"
                    }
                }
            },
            "members": {
                "operations": {
                    "MembersList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "groups",
                            "iterator": "groupGuid",
                            "path": "/api/v1/groups/{iterator}",
                            "base": {
                                "groupGuid": "{iterator}"
                            }
                        },
                        "processing_options": {
                            "output_field": null
                        },
                        "maxSessionCount": 20
                    }
                },
                "key": "groupGuid",
                "resources": {
                    "name": "string*",
                    "groupGuid": "string*",
                    "immutableId": "string*",
                    "isEnabled": "boolean*",
                    "isDefault": "boolean*",
                    "isQrEnabled": "boolean*",
                    "managedByUserGuid": "string*",
                    "users": [
                        "string*"
                    ],
                    "applications": [
                        "string*"
                    ],
                    "selfServiceProducts": [
                        "string*"
                    ],
                    "groups": [
                        "string*"
                    ],
                    "selfServiceCategories": [
                        "string*"
                    ]
                }
            },
            "groups": {
                "operations": {
                    "GroupsList": {
                        "method": "get",
                        "call": {
                            "path": "/api/v1/groups"
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    },
                    "UserCreate": {
                        "method": "post",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/groups/{groupGuid}"
                        },
                        "semantics": "create",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "isDeleted",
                            "groupGuid"
                        ],
                        "resource_mandatory": [
                            "name"
                        ]
                    },
                    "UserUpdate": {
                        "method": "put",
                        "call": {
                            "mode": "normal",
                            "path": "/api/v1/groups/{groupGuid}"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "isDeleted"
                        ],
                        "resource_mandatory": [
                            "groupGuid"
                        ]
                    }
                },
                "key": "groupGuid",
                "resources": {
                    "groupGuid": "string*",
                    "name": "string*",
                    "managedByUserGuid": "string*",
                    "isEnabled": "boolean*",
                    "isDefault": "boolean*",
                    "isDeleted": "boolean*",
                    "isQrEnabled": "boolean*"
                }
            },
            "roles": {
                "operations": {
                    "RolesList": {
                        "method": "get",
                        "call": {
                            "path": "/api/v1/roles"
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    }
                },
                "key": "roleGuid",
                "resources": {
                    "roleGuid": "string*",
                    "name": "string*",
                    "description": "string*",
                    "isEnabled": "boolean*",
                    "isDefault": "boolean*",
                    "isSystem": "boolean*",
                    "rights": [
                        "number*"
                    ],
                    "users": [
                        "string*"
                    ],
                    "groupGuids": [
                        "string*"
                    ]
                }
            },
            "applications": {
                "operations": {
                    "ApplicationsList": {
                        "method": "get",
                        "call": {
                            "path": "/api/v1/applications"
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    }
                },
                "key": "applicationGuid",
                "resources": {
                    "name": "string*",
                    "icon": "string*",
                    "url": "string*",
                    "helloIDUrl": "string*",
                    "isEnabled": "boolean*",
                    "type": "string*",
                    "description": "string*",
                    "applicationGuid": "string*",
                    "creationTime": "string*",
                    "modifyTime": "string*",
                    "modifyUser": "string*",
                    "links": [
                        {
                            "title": "_:string*",
                            "location": "_:string*"
                        }
                    ]
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://{tenant_id}",
            "get": {
                "pagination": {
                    "mode": "skip_take",
                    "page_size": "{page_size}",
                    "params": {
                        "take": "{take_count}",
                        "skip": "{skip_count}"
                    }
                },
                "selector": {
                    "mode": "objects",
                    "parameter": "orgs"
                }
            },
            "authentication": "basic_auth",
            "call_handling": "generic",
            "test_connection": {
                "url": "/api/v1/users?take=1&skip=0"
            },
            "accept": "application/json"
        }
    },
    "connection": {
        "items": [
            {
                "name": "tenant_id",
                "type": "textbox",
                "label": "Tenent URL:",
                "value": "customer.helloid.com",
                "order": 1
            },
            {
                "name": "user_name",
                "type": "textbox",
                "label": "API Key",
                "required": true,
                "order": 2
            },
            {
                "name": "password",
                "type": "textbox",
                "label": "API Secret",
                "required": true,
                "order": 3
            },

            {
                "name": "page_size",
                "type": "textbox",
                "label": "Page Size",
                "value": "1000",
                "order": 4
            }
        ]
    }
}